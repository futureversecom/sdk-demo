{
  "builds": [
    {
      "src": "apps/remix-demo/api/**/*.ts",
      "use": "@vercel/node"
    },
    {
      "src": "apps/remix-demo/app/**/*.{js,jsx,ts,tsx}",
      "use": "@vercel/remix"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "outputDirectory": "apps/remix-demo/public"
}
